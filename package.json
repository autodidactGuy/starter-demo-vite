{
  "name": "demo-starter",
  "description": "Demo Starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "node --experimental-json-modules --loader ts-node/esm ./scripts/dev/run.ts",
    "app:dev": "node --experimental-json-modules --loader ts-node/esm ./devtools/vite/scripts/startDevServer/run.ts",
    "build": "node --experimental-json-modules --loader ts-node/esm ./devtools/vite/scripts/buildApp/run.ts",
    "serve": "node --experimental-json-modules --loader ts-node/esm ./devtools/vite/scripts/runBuild/run.ts",
    "helpers:update-useweb-deps": "npm-check-updates -f /@useweb/*/ -u && npm i && npm run tests:ts-types",
    "tests:eslint": "eslint ./ --fix",
    "tests:ts-types": "tsc --noemit",
    "prettify": "node --loader  ts-node/esm ./devtools/prettier/scripts/runPrettier/runPrettier.ts",
    "install-deps": "npm i && cd functions && npm i"
  },
  "dependencies": {
    "@mui/material": "^5.8.4",
    "@tippyjs/react": "^4.2.6",
    "@useweb/click-to-component": "^2.2.1",
    "@useweb/ui": "^1.2.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.32.1",
    "react-location": "^3.3.4",
    "react-medium-image-zoom": "^4.4.3",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@check-light-or-dark/color": "^1.0.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@grpc/grpc-js": "^1.6.7",
    "@svgr/core": "^6.2.1",
    "@types/estree": "0.0.51",
    "@types/node": "^18.0.0",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.2",
    "chalk": "^4.1.2",
    "clipboardy": "^3.0.0",
    "command-line-args": "^5.2.1",
    "concurrently": "^7.2.2",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "dotenv-parse-variables": "^2.0.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.17.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.5.12",
    "filehound": "^1.17.6",
    "format-duration": "^2.0.0",
    "fs-extra": "^10.1.0",
    "history": "^5.3.0",
    "ink": "^3.2.0",
    "internal-ip": "^7.0.0",
    "kill-port-process": "^3.0.1",
    "live-server": "^1.2.2",
    "lodash": "^4.17.21",
    "node-emoji": "^1.11.0",
    "npm-check-updates": "^13.1.5",
    "ora": "^6.1.0",
    "prettier": "^2.7.1",
    "qrcode-terminal": "^0.12.0",
    "react-json-view": "^1.21.3",
    "rollup-plugin-manifest-json": "^1.5.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sharp": "^0.30.6",
    "style-loader": "^3.3.1",
    "svgo": "^2.8.0",
    "tcp-port-used": "^1.0.2",
    "ts-node": "^10.8.1",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0"
  }
